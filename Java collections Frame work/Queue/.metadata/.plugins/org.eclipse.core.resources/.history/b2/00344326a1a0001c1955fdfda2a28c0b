package queue;

import java.util.PriorityQueue;

public class EXTRAAddtofindMAXQueue {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		PriorityQueue<Integer> que = new PriorityQueue<Integer>() ;
		 
		que.add(111);
		que.add(121);
		que.add(500);
		que.add(141);
		que.add(151);
		
		var data = que.toArray();
		 
		int Max = (int) data[0];
		
		for(var d : data) {
			if(Max < (int)d);
			Max = (int) d;
		}
		System.out.println(Max);
		
	}

}
