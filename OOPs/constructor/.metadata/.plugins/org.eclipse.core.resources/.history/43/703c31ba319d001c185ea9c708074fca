package inherits;

import java.util.Scanner;

class  algebra {
   
	public  int  add(int a , int b ) {
		return a + b ;
	}
	
	public  int  mul(int a , int b ) {
		return  (a * b) ;
	}
}

class mul extends algebra{
	public int div(int a , int b ) {
		
		return  (a / b)  ;
		
	}
	
	



