package constructor;

class employee{
	String  name ;
	int id ;
	String address;
	int mob ;
	
	public void info (String name , int id , String address, int mob ) {
		this.name=name;
		this.id=id;
		this.address=address;
		this.mob=mob;
		
	}
	
}

