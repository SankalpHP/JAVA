package printbyforloop;

import java.util.Scanner;

public class byforloop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	
		for(int a = 1 ; a<= 5 ; a++) {
			
			Scanner no = new Scanner(System.in);
			a=no.nextInt();
			System.out.println("enter a valve");
		}
		
		

	}

}
